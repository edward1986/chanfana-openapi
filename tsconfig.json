{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "types": ["@types/node", "./worker-configuration.d.ts"],
    "noEmit": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "paths": {
      "firebase-admin/*": ["./node_modules/firebase-admin/lib/*"]
    }
  },
  "include": ["src"]
}
